    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.7.2",
    "react-router-dom": "^4.3.1",
    "serialize-javascript": "^1.6.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
