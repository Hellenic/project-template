  "repository": {
    "type": "git",
    "url": "https://github.com/Hellenic/project-template"
  },
  "scripts": {
    "dev": "razzle start",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node build/server.js --noninteractive",
    "build": "razzle build --noninteractive",
    "test": "razzle test --env=jsdom --coverage",
    "test:watch": "razzle test --env=jsdom --watch",
    "test:e2e": "razzle test -- test/e2e/",
    "prettify": "prettier src/** --write"
  },
  "dependencies": {
    "@jaredpalmer/after": "latest",
    "compression": "^1.7.4",
    "helmet": "^5.0.1",
    "express": "^4.17.2",
    "node-fetch": "^2.6.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.3.0",
    "react-intl": "^5.24.3",
    "serialize-javascript": "^6.0.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "babel-preset-razzle": "^4.2.14",
    "html-webpack-plugin": "^5.2.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nightmare": "^3.0.2",
    "postcss": "8.2.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "razzle": "^4.2.14",
    "razzle-dev-utils": "^4.2.14",
    "razzle-plugin-manifest": "^4.2.6",
    "react-test-renderer": "^17.0.2",
    "webpack": "5.65.0",
    "webpack-dev-server": "^3.11.2"
  },
  "engines": {
    "node": "14.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm audit"
    }
  }
}
